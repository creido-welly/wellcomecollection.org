{
  "parser": "babel-eslint",
  "plugins": [
    "flowtype",
    "import",
    "react"
  ],
  "extends": ["wellcome"],
  "rules": {
    "no-multi-spaces": "off",
    "no-multi-str": "off",
    "flowtype/define-flow-type": "warn",
    "no-duplicate-imports": "off",
    "import/no-duplicates": "error",
    "react/jsx-uses-react": "error",
    "react/jsx-uses-vars": "error"
  },
  "parserOptions": {
    "ecmaFeatures": {
      "jsx": true
    }
  }
}
