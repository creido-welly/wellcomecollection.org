<button {{ 'id='+ buttonId if buttonId }} class="btn {{ {s:'HNM5'} | fontClasses }} {{ modifierClasses }}" {{ attributes }}{% if eventTracking %} data-track-event="{{eventTracking}}"{% endif %}>
  {% if icon %}
    {% icon icon, null, extraIconClasses %}
  {% endif %}
  <span class="btn__text">{{text}}</span>
</button>
